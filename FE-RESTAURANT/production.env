# Production Environment Variables for Golden Bamboo Restaurant

# API Configuration
VITE_API_URL=https://api.goldenbamboo.com/api
VITE_API_TIMEOUT=10000

# Application Configuration
VITE_APP_NAME=Golden Bamboo Restaurant
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# Security
VITE_ENABLE_HTTPS=true
VITE_ENABLE_CSP=true

# Features
VITE_ENABLE_PWA=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# Performance
VITE_ENABLE_COMPRESSION=true
VITE_ENABLE_LAZY_LOADING=true
VITE_ENABLE_SERVICE_WORKER=true

# External Services
VITE_CLOUDINARY_URL=https://res.cloudinary.com/goldenbamboo
VITE_ANALYTICS_ID=GA_MEASUREMENT_ID
VITE_SENTRY_DSN=SENTRY_DSN_URL

# Cache Configuration
VITE_CACHE_TTL=300000
VITE_STATIC_CACHE_TTL=86400000

# Database
VITE_DB_POOL_SIZE=10
VITE_DB_TIMEOUT=5000





